@page
@model JS_WebBasics.Pages.typescriptBasicsModel
@{
}

<h2>TypeScript Basics</h2>
<div class="row">
    <div class="col-12">
        <hr />
        <h3 id="msg" class="text-primary"></h3>
        <hr />
    </div>
</div>
<div class="row">
    <div class="col-3">
        <button id="btn1" class="btn btn-primary"></button>
            Show the current millisecond.
    </div>
    <div class="col-3">
        <input type="radio" id="optblue" name="color" value="blue"/> 
        <label for="optblue">Blue</label>
    </div>
    <div class="col-3">
        <button id="btndivs" class="btn btn-danger">
            Changedivs
        </button>
    </div>
    <div class="col-3">
        <input type="radio" id="optred" name="color" value="red" />
        <label for="optred">Red</label>
    </div>
</div>
<script src="~/js/typescriptBasics.js" asp-append-version="true"> </script>